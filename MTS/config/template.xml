      <unit type="miliseconds"/>
    </param>
  </test>

  <!-- Current tests -->
  <test id="Powerfold">
    <name>Powerfold</name>
    <group>Current Tests</group>
    <description></description>
    <param id="Presence">
      <name>Test presence</name>
      <description></description>
      <type>bool</type>
      <value>true</value>
    </param>
    <param id="MaxCurrent">
      <name>Max current</name>
      <description></description>
      <type>int</type>
      <value>200</value>
      <min>0</min>
      <max>4000</max>
      <unit type="miliampheres"/>
    </param>
    <param id="MaxOverloadTime">
      <name>Max overload time</name>
      <description></description>
      <type>int</type>
      <value>200</value>
      <min>0</min>
      <max>5000</max>
      <unit type="miliseconds"/>
    </param>
    <param id="MaxTestingTime">
      <name>Max testing time</name>
      <description></description>
      <type>double</type>
      <value>3</value>
      <min>0</min>
      <max>10</max>
      <unit type="seconds"/>
    </param>
  </test>
  <test id="HeatingFoil">
    <name>Heating Foil</name>
    <group>Current Tests</group>
    <description></description>
    <param id="Presence">
      <name>Test presence</name>
      <description></description>
      <type>bool</type>
      <value>true</value>      
    </param>
    <param id="MinCurrent">
      <name>Min Current</name>
      <description></description>
      <type>int</type>
      <value>100</value>
      <min>0</min>
      <max>4000</max>
      <unit type="miliampheres"/>
    </param>
    <param id="MaxCurrent">
      <name>Max Current</name>
      <description></description>
      <type>int</type>
      <value>200</value>
      <min>0</min>
      <max>4000</max>
      <unit type="miliampheres"/>
    </param>
    <param id="TestingTime">
      <name>Testing Time</name>
      <description></description>
      <type>double</type>
      <value>2.5</value>
      <min>0</min>
      <max>20</max>
      <unit type="miliseconds"/>
    </param>
  </test>
  <test id="DirectionLight">
    <name>Direction Light</name>
    <group>Current Tests</group>
    <description></description>
    <param id="Presence">
      <name>Test presence</name>
      <description></description>
      <type>bool</type>
      <value>true</value>
    </param>
    <param id="MinCurrent">
      <name>Min Current</name>
      <description></description>
      <type>int</type>
      <value>100</value>
      <min>0</min>
      <max>4000</max>
      <unit type="miliampheres"/>
    </param>
    <param id="MaxCurrent">
      <name>Max Current</name>
      <description></description>
      <type>int</type>
      <value>200</value>
      <min>0</min>
      <max>4000</max>
      <unit type="miliampheres"/>
    </param>
    <param id="BlinksCount">
      <name>Number of Blinks</name>
      <description></description>
      <type>int</type>
      <value>2</value>
      <min>1</min>
      <max>10</max>
      <unit type="none"/>
    </param>
    <param id="LightingTime">
      <name>Lighting Time</name>
      <description></description>
      <type>double</type>
      <value>1.5</value>
      <min>0</min>
      <max>5</max>
      <unit type="seconds"/>
    </param>
    <param id="BreakTime">
      <name>Break Time</name>
      <description></description>
      <type>double</type>
      <value>1.5</value>
      <min>0</min>
      <max>5</max>
      <unit type="seconds"/>
    </param>
  </test>
  
  <!-- Presence test -->
  <test id="Rubber">
    <name>Rubber</name>
    <group>Presence Tests</group>
    <description>Test presence of rubber</description>
    <param id="Presence">
      <name>Is present</name>
      <description>If enabled rubber should be present</description>
      <type>bool</type>
      <value>true</value>
    </param>
  </test>
  <test id="MirrorGlass">
    <name>Mirror Glass</name>
    <group>Presence Tests</group>
    <description>Test presence of mirror glass</description>
    <param id="Presence">
      <name>Is present</name>
      <description>If enabled mirror glass should be present</description>
      <type>bool</type>
      <value>true</value>
    </param>
  </test>
        
</tests>