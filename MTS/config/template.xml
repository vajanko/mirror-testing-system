      <unit type="degrees"/>
    </param>
    <param id="MaxCurrent">
      <name>Max current</name>
      <description></description>
      <type>int</type>
      <value>200</value>
      <min>0</min>
      <max>2000</max>
      <unit type="miliampheres"/>
