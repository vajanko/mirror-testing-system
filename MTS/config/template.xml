    <param id="MaxOverloadTime">
      <name>Max overload time</name>
      <description></description>
      <type>int</type>
      <value>500</value>
      <min>0</min>
      <max>5000</max>
      <unit type="miliseconds"/>
    </param>
  </test>
  <test id="TravelSouth">
    <name>Travel South</name>
    <group>Travel Tests</group>
    <description></description>
    <param id="MinAngle">
      <name>Min angle</name>
      <description></description>
      <type>double</type>
      <value>5.0</value>
      <min>0</min>
      <max>15</max>
      <unit type="degrees"/>
    </param>
    <param id="MaxCurrent">
      <name>Max current</name>
      <description></description>
      <type>int</type>
      <value>200</value>
      <min>0</min>
      <max>2000</max>
      <unit type="miliampheres"/>
    </param>
    <param id="MaxOverloadTime">
      <name>Max overload time</name>
      <description></description>
      <type>int</type>
      <value>500</value>
      <min>0</min>
      <max>5000</max>
      <unit type="miliseconds"/>
    </param>
  </test>
  <test id="TravelNorth">
    <name>Travel North</name>
    <group>Travel Tests</group>
    <description></description>
    <param id="MinAngle">
      <name>Min angle</name>
      <description></description>
      <type>double</type>
      <value>5.0</value>
      <min>0</min>
