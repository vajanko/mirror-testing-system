<?xml version="1.0" encoding="utf-8" ?>

<!DOCTYPE tests SYSTEM "template.dtd">

<tests>
  <!-- Informative parameters about test collection -->
  <test id="Info">
    <name>Information</name>
    <group>Informative Settings</group>
    <description>This setting will not be used for testing.</description>
    <param id="orientation">
      <name>Orientation</name>
      <description></description>
      <type>enum</type>
      <value>0</value>      <!-- index of selected value -->
      <values>
        <value>Left</value>
        <value>Right</value>
      </values>
    </param>
    <param id="partNumber">
      <name>Part Number</name>
      <description></description>
      <type>string</type>
      <value></value>
    </param>
    <param id="supplierName">
      <name>Supplier Name</name>
      <description></description>
      <type>string</type>
      <value></value>      
    </param>
    <param id="description">
      <name>Description</name>
      <description></description>
      <type>string</type>
      <value></value>
    </param>
  </test>
  
  <!-- Tear test -->
  <test id="TearTest" enabled="true">
    <name>Tear Glass Test</name>
    <group>Other</group>
    <description></description>
    <param id="TestingTime">
      <name>Testing Time</name>
      <description>Time of glass tearing out</description>
      <type>double</type>
      <value>2.5</value>
      <min>0</min>
      <max>20</max>
      <unit type="miliseconds"/>
    </param>
  </test>        
  
  <!-- Travel tests -->
  <test id="TravelEast">
    <name>Travel East</name>
    <group>Travel Tests</group>
    <description></description>
    <param id="MinAngle">
      <name>Min angle</name>
      <description></description>
      <type>double</type>
      <value>5.0</value>
      <min>0</min>
      <max>15</max>
      <unit type="degrees"/>
    </param>
    <param id="MaxCurrent">
      <name>Max current</name>
      <description></description>
      <type>int</type>
      <value>200</value>
      <min>0</min>
      <max>2000</max>
      <unit type="miliampheres"/>
    </param>
    <param id="MaxOverloadTime">
      <name>Max overload time</name>
      <description></description>
      <type>int</type>
      <value>500</value>
      <min>0</min>
      <max>5000</max>
      <unit type="miliseconds"/>
    </param>
  </test>
  <test id="TravelWest">
    <name>Travel West</name>
