<UserControl x:Class="MTS.Controls.Presence"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ed="clr-namespace:MTS.EditorModule"
    xmlns:ctrl="clr-namespace:MTS.Controls"
             BorderBrush="Black" BorderThickness="1" Width="293" Height="192">
    <StackPanel>
        <ed:TestControl TestName="My Test Name" Height="100" x:Name="test">
            <StackPanel>
                <TextBox Text="{Binding ElementName=test, Path=TestName}"/>
                <CheckBox>some check box</CheckBox>
            </StackPanel>
        </ed:TestControl>            
        <ctrl:UpDownButton/>
        <ctrl:NumericBox Text="12.00"/>
    </StackPanel>
</UserControl>
